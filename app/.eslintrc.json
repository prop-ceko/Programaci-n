{
   "env": {
      "browser": true,
      "es2022": true
   },
   "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/recommended",
      "eslint-config-prettier"
   ],
   "overrides": [],
   "parserOptions": {
      "ecmaFeatures": { "jsx": true },
      "ecmaVersion": "latest",
      "sourceType": "module"
   },
   "plugins": ["react"],
   "settings": {
      "react": {
         "version": "detect"
      },
      "import/resolver": {
         "node": {
            "paths": ["src"],
            "extensions": [".js", ".jsx"]
         }
      }
   },
   "rules": {
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "react/display-name": "off",
      "no-unused-vars": "warn"
   }
}
